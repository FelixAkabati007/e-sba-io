{
  "name": "e-sba",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server:dev": "tsx server/index.ts",
    "server:start": "node dist/server/index.js",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "server:build": "tsc -p tsconfig.server.json",
    "build:exe": "npm run build && npm run server:build && pkg dist/server/server/index.js --out-path build --targets node18-win-x64 --options no-bytecode",
    "installer:nsis": "makensis installer\\E-SBA.nsi",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --max-warnings=0 .",
    "test": "vitest run",
    "test:ui": "vitest",
    "e2e": "playwright test",
    "db:reset": "tsx server/scripts/reset-db.ts",
    "db:clean": "tsx server/scripts/clean-master-db.ts",
    "db:supabase:deploy": "tsx server/scripts/deploy-supabase-schema.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "image-size": "^1.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "lucide-react": "^0.469.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^5.4.8",
    "vitest": "^2.1.5"
  }
