{
  "version": 2,
  "buildCommand": "npm run build && npm run server:build",
  "outputDirectory": "dist",
  "framework": null,
  "builds": [
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "dist/server/index.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
